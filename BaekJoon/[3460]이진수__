#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <vector>

using namespace std;

int main() {
	//freopen("input.txt", "r", stdin);

	int testCase, num;
	vector<int> v;

	cin >> testCase;

	for (int test = 0; test < testCase; test++) {
		cin >> num;

		while (1) {
			v.push_back(num % 2);
			num /= 2;
			if (num == 1) {
				v.push_back(1);
				break;
			}
		}
		for (int i = 0; i < v.size(); i++) {
			if (v[i] == 1) {
				cout << i << " ";
			}
		}
		v.clear();
	}

	return 0;
}
