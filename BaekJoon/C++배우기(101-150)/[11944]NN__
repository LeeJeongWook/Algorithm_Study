#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <stack>
#include <vector>
using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	freopen("input.txt", "r", stdin);

	int N, M, tmp, cnt = 0;
	stack<int> s;
	vector<int> v;

	cin >> N >> M;

	if (M > N)
		M = N;

	while (N != 0) {
		tmp = N % 10;
		N /= 10;
		s.push(tmp);
	}

	for (int i = 0; i < (s.size()+1); i++) {
		v.push_back(s.top());
		s.pop();
	}

	while (cnt < M) {
		for (int x : v) {
			cout << x;
			cnt++;
		}
	}

	return 0;
}
