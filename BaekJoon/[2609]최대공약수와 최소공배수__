#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
	int A, B, tmp, div, mul;
	vector<int> v;

	cin >> A >> B;

	if (A > B) 
		tmp = B;
	else 
		tmp = A;

	for (int i = 1; i * i <= tmp; i++) {
		if (tmp % i == 0) {
			v.push_back(i);
			v.push_back(tmp/i);
		}
	}

	sort(v.begin(), v.end(), greater<int>());
	
	for (int i = 0; i < v.size(); i++) {
		if (A % v[i] == 0) {
			div = v[i];
			break;
		}
	}
	tmp = A * B;
	mul = tmp / div;

	cout << div << endl << mul;

	return 0;
}
