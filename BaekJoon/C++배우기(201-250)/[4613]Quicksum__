#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <string>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	freopen("input.txt", "r", stdin);

	string str;

	while (getline(cin, str)) {
		if (str == "#")
			break;
		int ans = 0;
		for (int i = 1; i <= str.size(); i++) {
			int tmp = str[i] - 64;
			if ((1 <= str[i]) && (str[i] < 27)) {
				ans += i * tmp;
			}
			
		}
		cout << ans << endl;
		
	}
	

	return 0;
}
