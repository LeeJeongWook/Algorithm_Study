#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <vector>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	freopen("input.txt", "r", stdin);

	int T, N, tmp;

	cin >> T;

	for (int test = 0; test < T; test++) {
		bool ans = true;
		vector<int> a, b, c;

		cin >> N;

		while (N) {
			cout << N % 10 << endl;
			b.push_back(N % 10);
			N /= 10;
		}

		for (int i = b.size() - 1; i >= 0; i--) {
			a.push_back(b[i]);
		}

		for (int i = 0; i < a.size(); i++) {
			tmp = a[i] + b[i];
			if (tmp < 10) {
				c.push_back(tmp);
			}
			else {
				c.push_back(tmp % 10);
				tmp /= 10;
				c.push_back(tmp % 10);
			}
		}

		for (int x : c) {
			cout << x << " ";
		}
		cout << endl;

		if (ans == false) {
			cout << "NO" << '\n';
		}
		else {
			cout << "YES" << '\n';
		}
	}

	return 0;
}
